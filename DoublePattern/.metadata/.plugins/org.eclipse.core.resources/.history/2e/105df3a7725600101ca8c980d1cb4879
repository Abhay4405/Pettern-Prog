package P15;

public class P15 {
public static void main(String[] args) {
	for (int i = 0; i < 4; i++) {
		for (int j = 0; j < 5; j++) {
			if (i >= j) {
				System.out.print("*");
			}
		}
		System.out.println();
	}
	for (int i = 4; i >= 0; i--) {
		for (int j = 0; j < 5; j++) {
			if (i>=j) {
				System.out.print("*");
			}
		}
		System.out.println();
	}
	for (int i = 4; i >= 0; i--) {
		for (int j = 0; j < 5; j++) {
			if (j>=i) {
				System.out.print("*");
			}
			else {
				System.out.print(" ");
			}
		}
		System.out.println();
	}
}
}
