package P17;

public class P17 {
	public static void main(String a[]) {

		for (int i = 1; i <= 3; i++) {
			for (int j = 1; j <= 5; j++) {
				if (j <= i || i + j >= 6) {
					System.out.print("*");
				} else {
					System.out.print(" ");
				}
			}
			System.out.println();
		}

		for (int i = 1; i <= 3; i++) {
			for (int j = 1; j <= 5; j++) {
				if (j <= i || i + j >= 6) {
					System.out.print("*");
				} else {
					System.out.print(" ");
				}
			}
			System.out.println();
		}

	}

}
