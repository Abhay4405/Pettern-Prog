package P18;

public class P18 {
public static void main(String[] args) {
	for (int i = 0; i < 3; i++) {
		for (int j = 0; j < 6; j++) {
			if (i>=j||i+j>=4) {
				System.out.print("*");
			}
			else {
				System.out.print(" ");
			}
		}
		System.out.println();
	}
	for (int i = 3; i>= 0; i--) {
		for (int j = 0; j < 6; j++) {
			if (i>=j||i+j>=4) {
				System.out.print("*");
			}
			else {
				System.out.print(" ");
			}
		}
		System.out.println();
	}
}
}
