package P11;

public class P11 {
public static void main(String[] args) {
	for (int i = 0; i < 5; i++) {
		for (int j = 0; j < 5; j++) {
			if (i==4||j==0||i==j) {
				System.out.print("*");
			}
			else {
				System.out.print(" ");
			}
		}
		System.out.println();
	}
	for (int i = 4; i >= 0; i--) {
		for (int j = 0; j < 5; j++) {
			if (i==4||j==0||i==j) {
				System.out.print("*");
			}
			else {
				System.out.print(" ");
			}
		}
		System.out.println();
	}
}
}
